
Public main class()
for(int i=0;i<inputArray.length;i++){
if(inputArray[i] == key){
return i;
}
}
return -1;
int index = i;
for (int j = i + 1; j < arraySize; j++) {
if (inputArray[j] < inputArray[index]){
index = j;
}
}
int temp = inputArray[i];
inputArray[i] = inputArray[index];
inputArray[index] = temp;
}
